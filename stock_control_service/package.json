{
    "name": "stockcontrol_service",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "migration": "npx prisma migrate dev --create-only",
        "seed": "npx prisma db seed",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "tsc": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "build:clean": "rimraf dist",
        "build": "npx prisma migrate dev && npm run build:clean && tsc",
        "build:dev": "tsc -w",
        "start": "npx prisma migrate dev && node dist/src/main.js",
        "start:build": "npm run build && npm start",
        "start:dev": "npx prisma migrate dev && ts-node -r tsconfig-paths/register src/main.ts",
        "start:prod": "npx prisma migrate dev && node dist/main"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@prisma/client": "^5.22.0",
        "amqplib": "^0.10.4",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-validator": "^7.2.0",
        "rimraf": "^6.0.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "tslib": "^2.8.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/amqplib": "^0.10.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.9.0",
        "@types/supertest": "^6.0.2",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.7",
        "@typescript-eslint/eslint-plugin": "^8.14.0",
        "@typescript-eslint/parser": "^8.14.0",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "jest-mock-extended": "^4.0.0-beta1",
        "prettier": "^3.3.3",
        "prisma": "^5.22.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    }
}
